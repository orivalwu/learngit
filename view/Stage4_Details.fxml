<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.S4_Controller">
    <children>
        <VBox>
            <children>
                <Label alignment="CENTER" prefHeight="65.0" prefWidth="686.0" text="各选手参赛详情">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
                <TableView fx:id="details">
                    <columns>
                        <TableColumn id="id" fx:id="TC_id" prefWidth="75.0" text="id" />
                        <TableColumn id="order" fx:id="TC_competition" prefWidth="151.0" text="场次" />
                        <TableColumn id="specific" fx:id="TC_info" prefWidth="239.0" text="详情" />
                        <TableColumn id="total" fx:id="TC_grade" prefWidth="75.0" text="总成绩" />
                        <TableColumn id="rank" fx:id="TC_level" prefWidth="122.0" text="排名" />
                    </columns>
                </TableView>
                <Pane>
                    <children>
                        <Button fx:id="info" mnemonicParsing="false" onAction="#finalPlay" prefHeight="57.0" prefWidth="161.0" text="精英赛角逐">
                            <font>
                                <Font size="24.0" />
                            </font>
                        </Button>
                        <Button fx:id="exit" layoutX="597.0" layoutY="-1.0" mnemonicParsing="false" onAction="#exit" prefHeight="57.0" prefWidth="89.0" text="退出">
                            <font>
                                <Font size="24.0" />
                            </font>
                        </Button>
                  <Button fx:id="save" layoutX="332.0" layoutY="1.0" mnemonicParsing="false" onAction="#saving" prefHeight="57.0" prefWidth="89.0" text="保存">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                    </children>
                </Pane>
            </children>
        </VBox>
    </children>
</AnchorPane>
