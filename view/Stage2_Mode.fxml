<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.S2_Controller">
    <children>
        <Pane layoutY="-7.0" prefHeight="505" prefWidth="691.0" AnchorPane.bottomAnchor="7.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="-7.0">
            <children>
                <ImageView fitHeight="525.0" fitWidth="691.0" layoutX="-1.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../imageResource/boil.jpg" />
                    </image>
                </ImageView>
                <Button fx:id="singleGame" layoutX="34.0" layoutY="389.0" mnemonicParsing="false" onAction="#setSingleGame" text="单人模式">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="doubleGame" layoutX="193.0" layoutY="389.0" mnemonicParsing="false" onAction="#setDoubleGame" text="双人模式">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="tripleGame" layoutX="322.0" layoutY="389.0" mnemonicParsing="false" onAction="#setTripleGame" text="三人模式">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="pentaGame" layoutX="486.0" layoutY="389.0" mnemonicParsing="false" onAction="#setPentaGame" text="五人模式">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="omniunGame" layoutX="100.0" layoutY="451.0" mnemonicParsing="false" onAction="#setomniunGame" prefHeight="36.0" prefWidth="102.0" text="全能赛">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="eliteGame" layoutX="289.0" layoutY="451.0" mnemonicParsing="false" onAction="#setEliteGame" prefHeight="36.0" prefWidth="96.0" text="精英赛">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="back" layoutX="452.0" layoutY="451.0" mnemonicParsing="false" onAction="#back" text="返回主菜单">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>

            </children>
        </Pane>
    </children>
</AnchorPane>
